@model IEnumerable<SavingOps.Models.Rent>    

    <table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.DateSubmitted)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Cost)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
               @{
                        string[] date = item.DateSubmitted.ToString().Split();
                        @date[0]
                    }
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Cost)
            </td>            
        </tr>
}
    </tbody>
</table>
