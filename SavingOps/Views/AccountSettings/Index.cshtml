@model IEnumerable<SavingOps.Models.AccountSettings>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.RentPrice)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.SavingsGoal)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.RentPrice)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.SavingsGoal)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.AccountSettingsID">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.AccountSettingsID">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.AccountSettingsID">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
